{
    "env": {
      "browser": true,
      "es6": true
    },
    "plugins": ["import"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error",
      "import/first": "error",
      "semi": ["warn", "never"],
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
    }
  }